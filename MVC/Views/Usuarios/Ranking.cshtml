@model IEnumerable<MVC.Models.Usuario>

@{
    ViewBag.Title = "Ranking usuarios";
}

<h2 class="text-center">Ranking usuarios</h2>
@if (Model.Count() > 0) { 
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Puntaje)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NombreUsuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Puntaje)
                </td>
            </tr>
        }

    </table>
    }